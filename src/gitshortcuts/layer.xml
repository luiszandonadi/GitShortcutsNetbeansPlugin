<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Other">
            <file name="gitshortcuts-OpenGitGui.instance"/>
            <file name="gitshortcuts-OpenGitk.instance"/>
        </folder>
        <folder name="Versioning">
            <file name="gitshortcuts-GitInitAction.instance"/>
            <file name="gitshortcuts-GitStatusAction.instance"/>
            <file name="gitshortcuts-GitDiffAction.instance"/>
            <file name="gitshortcuts-GitCheckoutAction.instance"/>
        </folder>
    </folder>
    <folder name="Toolbars">
        <folder name="File">
            <file name="gitshortcuts-OpenGitGui.shadow">
                <attr name="originalFile" stringvalue="Actions/Other/gitshortcuts-OpenGitGui.instance"/>
                <attr name="position" intvalue="0"/>
            </file>
            <file name="gitshortcuts-OpenGitk.shadow">
                <attr name="originalFile" stringvalue="Actions/Other/gitshortcuts-OpenGitk.instance"/>
                <attr name="position" intvalue="-100"/>
            </file>
        </folder>
    </folder>
    <folder name="Shortcuts">
        <file name="C-A-g.shadow">
            <attr name="originalFile" stringvalue="Actions/Other/gitshortcuts-OpenGitGui.instance"/>
        </file>
        <file name="C-A-k.shadow">
            <attr name="originalFile" stringvalue="Actions/Other/gitshortcuts-OpenGitk.instance"/>
        </file>
        <file name="C-A-s.shadow">
            <attr name="originalFile" stringvalue="Actions/Versioning/gitshortcuts-GitStatusAction.instance"/>
        </file>
        <file name="C-A-d.shadow">
            <attr name="originalFile" stringvalue="Actions/Versioning/gitshortcuts-GitDiffAction.instance"/>
        </file>
    </folder>
       <folder name="Menu">
        <folder name="Versioning">
            <attr name="gitshortcuts-GitInitAction.shadow/org-netbeans-modules-versioning-VersioningMainMenu.instance" boolvalue="true"/>
            <file name="gitshortcuts-GitInitAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Versioning/gitshortcuts-GitInitAction.instance"/>
            </file>
            <file name="gitshortcuts-GitStatusAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Versioning/gitshortcuts-GitStatusAction.instance"/>
            </file>
            <file name="gitshortcuts-GitDiffAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Versioning/gitshortcuts-GitDiffAction.instance"/>
            </file>
            <file name="gitshortcuts-GitCheckoutAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Versioning/gitshortcuts-GitCheckoutAction.instance"/>
            </file>
        </folder>
    </folder>
</filesystem>
